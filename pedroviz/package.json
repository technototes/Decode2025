  "type": "module",
  "scripts": {
    "format": "prettier -w \"*.ts*\" \"client/**/*.tsx\" \"server/**/*.ts\" \"*.md\" \"*.json\" \"*.html\"",
    "open": "bun run os open",
    "open:win": "cmd /c start \"\"",
    "open:mac": "open",
    "open:lin": "xdg-open",
    "os": "bun run scripts/os.ts",
    "types": "tsc --noEmit -p tsconfig.json",
    "pvdev": "bun --hot index.tsx",
    "pvbuild": "bun build ./index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "pvstart": "NODE_ENV=production bun index.tsx"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.72.8",
    "@fluentui/react-icons": "^2.0.316",
    "@freik/containers": "^0.4.4",
    "@freik/fluentui-tools": "^1.2.4",
    "@freik/react-tools": "^0.3.4",
    "@freik/typechk": "^0.9.0",
    "java-parser": "^3.0.1",
    "jotai": "^2.16.0",
    "jotai-family": "^1.0.1",
    "prettier": "^3.7.4",
    "prettier-plugin-java": "^2.7.7",
    "prettier-plugin-organize-imports": "^4.3.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^20.0.11",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "1.3.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  },
  "private": true
}
